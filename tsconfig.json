{
    "compilerOptions": {
        "baseUrl": "./src",
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "outDir": "./public/",
        "sourceMap": true,
        "noImplicitAny": true,
        "lib": [
            "dom",
            "ES2021.String"
        ],
        "module": "es2015",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
    }
}